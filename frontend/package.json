{
  "name": "directus-3d-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.ts,.vue --config .eslintrc.cjs",
    "lint:fix": "eslint . --ext .js,.ts,.vue --config .eslintrc.cjs --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "check": "yarn lint && yarn type-check",
    "i18n:check": "NODE_OPTIONS='--loader ts-node/esm' ts-node scripts/i18n-check.ts",
    "i18n:report": "NODE_OPTIONS='--loader ts-node/esm' ts-node scripts/i18n-check.ts --report"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.44.1",
    "pinia": "^3.0.3",
    "three": "^0.177.0",
    "vite": "^6.3.5",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "@types/three": "^0.177.0",
    "@types/vue": "^2.0.0",
    "@types/vue-i18n": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/runtime-core": "^3.5.16",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "glob": "^11.0.2",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vercel": "^42.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.0",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.2.10",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}
