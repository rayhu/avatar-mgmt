{
  "version": 2,
  "name": "avatar-mgmt",
  "functions": {
    "api-server/index.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api-server/index.ts"
    },
    {
      "src": "/health",
      "dest": "/api-server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "regions": ["hkg1"]
}
